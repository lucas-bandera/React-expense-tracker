{
  "name": "react-expense-tracker",
  "version": "1.0.0",
  "description": "Simple react expense tracker app integrated with a mondoDB  backend",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "npm run install-client && npm ruin build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasBandeira-MJ/React-expense-tracker.git"
  },
  "keywords": [],
  "author": "Lucas Bandeira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LucasBandeira-MJ/React-expense-tracker/issues"
  },
  "homepage": "https://github.com/LucasBandeira-MJ/React-expense-tracker#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
